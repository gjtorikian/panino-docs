#!/usr/bin/env node
var panino = require('../lib/cli');

// Not from the outside? Then it's from the command line
if (module.parent === null) {
    panino.main(process.argv.slice(2), function (err) {
        if (err) {
            console.error(err);
            process.exit(1);
        }
        process.exit(0);
    });
}