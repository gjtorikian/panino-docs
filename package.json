                        "minimatch"   : "0.2.9",
                        "jade"        : "0.30.0",
                        "marked"      : "0.2.9",
                        "underscore"  : "1.4.4",
                        "fs-tools"    : "0.2.10",
                        "wrench"      : "1.5.1",
                        "markdown_conrefs": "1.2.17",
                        "functional-docs": "0.1.18",
                        "render-html-from-ast": "0.0.1",
                        "esprima":    "1.0.3",
                        "StringScanner": "0.0.3",
                        "colors": "0.6.0",
                        "function-extractor": "0.0.4"
                      },
  "devDependencies" : { "mocha": "1.11.0" },

  "engines"         : { "node": ">= 0.10.0" },

  "preferGlobal"    : true
}
