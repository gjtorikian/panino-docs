  "keywords"        : [
                        "api", "doc", "apidoc", "documentation", "pdoc", "ndoc"
                      ],
  "homepage"        : "https://github.com/gjtorikian/panino",

  "author"          : "Garen J. Torikian <gjtorikian@gmail.com>",
  "contributors"   : [
